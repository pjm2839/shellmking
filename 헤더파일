#include<stdio.h>
   #include<stdlib.h>//system()함수가 있음
   #include<unistd.h>// fork()함수가 있음
   #include<stdlib.h>
   #include<signal.h>
   #include<sys/type.h>
   #include<setjmp.h>
  
   struct superblock{
      //inode 개수랑 datablock사용여부를 담아야함.
  };
 
  struct inode{
      char filesort[5];//파일의 형식 디렉토리인지 일반파일인지
      int time_made;// 파일이 만들어진 시간
      int b= sizeof(*a);//a(포인터)는 파일을 가리킴 그 사이즈를 b가 저장.
  };
 
  void myls(**argvp);
  void mycat(**argvp);
  void mypwd(**argvp);
  void mycd(**argvp);
  void mycp(**argvp);
  void mycpto(**argvp);
  void mycpform(**argvp);
  void mymkdir(**argvp);
  void myrmdir(**argvp);
  void mymv(**argvp);
  void mytouch(**argvp);
  void myshowinode(**argvp);
  void myshowblock(**argvp);
  void mystate(**argvp);
  void mytree(**argvp);
  void command(**argvp);
